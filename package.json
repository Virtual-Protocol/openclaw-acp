{
  "name": "virtuals-protocol-acp",
  "version": "0.2.0",
  "description": "Agent Commerce Protocol (ACP) CLI â€” wallet, marketplace, token, and seller runtime for AI agents",
  "type": "module",
  "bin": {
    "acp": "./bin/acp.ts"
  },
  "scripts": {
    "acp": "tsx bin/acp.ts",
    "test": "npm run seller:dry-run -- --both",
    "test:listener-stack": "tsx scripts/repro_listener_payload_mismatch.ts && tsx scripts/test-listener-stack.ts",
    "test:all": "npm run test:listener-stack && npm run seller:dry-run -- --both",
    "setup": "tsx bin/acp.ts setup",
    "offering:create": "tsx bin/acp.ts sell create",
    "offering:delete": "tsx bin/acp.ts sell delete",
    "resource:create": "tsx bin/acp.ts sell resource create",
    "resource:delete": "tsx bin/acp.ts sell resource delete",
    "seller:run": "tsx bin/acp.ts serve start",
    "seller:stop": "tsx bin/acp.ts serve stop",
    "seller:check": "tsx bin/acp.ts serve status",
    "seller:dry-run": "tsx scripts/dry-run-delivery.ts",
    "listener:start": "bash scripts/listener-stack.sh start",
    "listener:stop": "bash scripts/listener-stack.sh stop",
    "listener:status": "bash scripts/listener-stack.sh status",
    "listener:health": "bash scripts/listener-stack.sh health",
    "listener:doctor": "bash scripts/listener-stack.sh doctor",
    "listener:test": "bash scripts/listener-stack.sh test"
  },
  "dependencies": {
    "axios": "^1.13.4",
    "dotenv": "^16.4.5",
    "socket.io-client": "^4.8.1"
  },
  "devDependencies": {
    "tsx": "^4.19.2",
    "typescript": "^5.7.2"
  }
}
